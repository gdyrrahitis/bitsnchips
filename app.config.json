{
    "staticResources": {
        "directories": [
            {
                "path": "src",
                "order": 1
            },
            {
                "path": "node_modules",
                "order": 2
            },
            {
                "path": "dir",
                "order": 3
            }
        ],
        "entry": "index.html"
    },
    "server": {
        "port": 54879
    },
    "socketio": {
        "maxRoomsAllowed": 1
    },
    "client": {
        "baseUrl": "http://localhost:54879",
        "basePath": "/",
        "html5Mode": true,
        "routes": [
            {
                "path": "/",
                "templateUrl": "home.controller/home.controller.html",
                "controller": "homeController"
            },
            {
                "path": "/room/:id",
                "templateUrl": "room.controller/room.controller.html",
                "controller": "roomController"
            }
        ]
    },
    "poker": {
        "mountainGoat": ["zero", "half", "one", "two", "three", "five", "eight", "thirteen", "twenty", "forty", "one-hundred", "coffee", "question"]
    }
}