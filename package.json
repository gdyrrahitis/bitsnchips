{
  "name": "pcard",
  "version": "1.0.0",
  "description": "A scrum poker planning real-time application",
  "main": "gulpfile.js",
  "scripts": {
    "test": "tsc && karma start karma.conf.js",
    "test:dev": "gulp test:dev",
    "test:travis": "gulp test:travis",
    "test:e2e:dev": "tsc && protractor src/e2e/conf.js",
    "test:express:dev": "mocha \"./src/server/tests/**/*.js\" --timeout 10000 --watch",
    "test:express:travis": "mocha \"./src/server/tests/**/*.js\" --timeout 10000",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "gulp app:dev",
    "start:prod": "gulp app:prod && npm run server",
    "server": "node app.js"
  },
